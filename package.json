  },
  "peerDependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "devDependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "sagui": "^7.4.0"
  }
